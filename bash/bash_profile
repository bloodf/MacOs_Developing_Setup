export PATH="/usr/local/sbin:$PATH"

source ~/.profile

# Android Configurations
if [ -f ~/.android ]; then
  source ~/.android
fi

# Flutter Configurations
if [ -f ~/.flutter ]; then
  source ~/.flutter
fi

# NVM Auto Load
if [ -f ~/.nvm-load ]; then
  source ~/.nvm-load
fi

#ALIAS
if [ -f ~/.bash_alias ]; then 
    . ~/.bash_alias;
fi